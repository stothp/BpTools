<Window x:Class="BpToolsWPFClientTest.DataRenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BpToolsWPFClientTest"
        mc:Ignorable="d"
        Title="Refactor rename" Height="135.956" Width="291.012">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Name" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Right" Margin="0,0,10,0"/>
        <TextBox Name="DataNameTextBox" Text="{Binding DataName}" Grid.Column="1" Grid.Row="0" />
        <WrapPanel Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" Margin="0,10,0,0" Width="Auto" Height="Auto" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Button IsDefault="True" Content="Ok" MinWidth="60" Margin="10,0,0,0" Click="Button_Click"></Button>
            <Button IsCancel="True" Content="Cancel" MinWidth="60" Margin="10,0,0,0"></Button>
        </WrapPanel>
    </Grid>
</Window>
