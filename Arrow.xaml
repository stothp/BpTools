<UserControl x:Class="BpToolsWPFClientTest.Arrow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BpToolsWPFClientTest"
             mc:Ignorable="d" 
             d:DesignHeight="10" d:DesignWidth="10">
    <Grid>
        <Path Name="ArrowPath" Stroke="Gray" StrokeThickness="1" Fill="Gray"
              Data = "M0,0 L5,5 L0,10 Z" RenderTransformOrigin="0.5, 0.5">
        </Path>
    </Grid>

</UserControl>
